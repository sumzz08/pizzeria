{
    "version":2,
    "builds":[
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methoods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}